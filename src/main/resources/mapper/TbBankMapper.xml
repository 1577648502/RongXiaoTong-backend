<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lfg.rongxiaotong.mapper.TbBankMapper">

    <resultMap id="BaseResultMap" type="com.lfg.rongxiaotong.domain.TbBank">
            <id property="bankId" column="bank_id" jdbcType="INTEGER"/>
            <result property="bankName" column="bank_name" jdbcType="VARCHAR"/>
            <result property="introduce" column="introduce" jdbcType="VARCHAR"/>
            <result property="bankPhone" column="bank_phone" jdbcType="VARCHAR"/>
