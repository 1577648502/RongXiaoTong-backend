            <result property="rate" column="rate" jdbcType="DECIMAL"/>
            <result property="repayment" column="repayment" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        bank_id,bank_name,introduce,
        bank_phone,money,rate,
        repayment
